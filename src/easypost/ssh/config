Host */*
    ProxyCommand ssh -qax $(dirname %h) nc6 -q 1 -w 1 $(basename %h) %p

Host admin
    User jhammond
    ForwardAgent yes
    HostName admin.easypo.net

Host ep-vm*
    User vagrant
    ForwardAgent yes

Host *.easypo.net
    User jhammond
    ForwardAgent yes

Host *sj *wdc *nuq
    User jhammond
    ForwardAgent yes
    ProxyJump admin.easypo.net
    ProxyCommand ssh -qax admin nc6 -q 1 -w 1 %h %p

Host *
    VisualHostKey yes
    ForwardAgent no
    UserKnownHostsFile ~/.ssh/known_hosts
    SendEnv TZ
    ServerAliveInterval 180

Host oregon2
    HostName oregon2.jhammond.devvm.easypo.net
    User vagrant
    ForwardAgent yes
    ProxyJump jhammond@admin.dev.easypo.net
    IdentityFile ~/.ssh/config
